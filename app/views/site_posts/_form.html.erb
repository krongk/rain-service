<%= simple_form_for(@site_post) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :user %>
    <%= f.association :site %>
    <%= f.input :title %>
    <%= f.input :content %>
    <%= f.input :key_words %>
    <%= f.input :cate_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
