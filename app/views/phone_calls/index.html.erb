<h1>Listing phone_calls</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Domain</th>
      <th>From ip</th>
      <th>From url</th>
      <th>From phone</th>
      <th>Is processed</th>
      <th>Note</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @phone_calls.each do |phone_call| %>
      <tr>
        <td><%= phone_call.user %></td>
        <td><%= phone_call.domain %></td>
        <td><%= phone_call.from_ip %></td>
        <td><%= phone_call.from_url %></td>
        <td><%= phone_call.from_phone %></td>
        <td><%= phone_call.is_processed %></td>
        <td><%= phone_call.note %></td>
        <td><%= link_to 'Show', phone_call %></td>
        <td><%= link_to 'Edit', edit_phone_call_path(phone_call) %></td>
        <td><%= link_to 'Destroy', phone_call, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Phone call', new_phone_call_path %>
