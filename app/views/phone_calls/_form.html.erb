<%= simple_form_for(@phone_call) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :domain, as: :hidden %>
    <%= f.input :from_ip, as: :hidden %>
    <%= f.input :from_url, as: :hidden %>
    <%= f.input :from_phone %>
    <%#= f.input :is_processed %>
    <%#= f.input :note %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, data: { disable_with: "正在提交..." }  %>
  </div>
<% end %>
