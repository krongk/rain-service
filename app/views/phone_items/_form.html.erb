<%= simple_form_for(@phone_item) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%#= f.association :user %>
    <%= f.input :name %>
    <%= f.input :mobile_phone %>
    <%#= f.input :source_name %>
    
    <%#= f.input :city %>
    <%#= f.input :area %>
    <%#= f.input :description %>
    <%#= f.input :is_processed %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, data: { disable_with: '正在提交...'} %>
  </div>
<% end %>
